{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/views/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/app.js"
    }
  ]
}