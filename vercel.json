{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "/dist/views/**",
      "use": "@vercel/static"
    },
    {
      "src": "dist/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/dist/views/css/$1"
    },
    {
      "src": "/widget.html",
      "dest": "/dist/views/widget.html"
    },
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ]
}
