{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "src/views/css/$1" },
    { "src": "/api/.*", "dest": "src/app.js" },
    { "src": "/auth/.*", "dest": "src/app.ts" },
    { "src": "/api/widget/.*", "dest": "src/app.ts" },
    { "src": "/.*", "dest": "src/app.ts" }
  ]
}